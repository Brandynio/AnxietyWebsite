@model Serenity.Models.ActivityModel

@{
    ViewBag.Title = "RateActivity";
}

<h2>Rate @Model.ActivityName</h2>

<div id="help">
    <p>1. Did this activity help you?</p>

</div>

<div id="ratingsDiv">
    <div class="ratingBox" id="rate1">1</div>
    <div class="ratingBox" id="rate2">2</div>
    <div class="ratingBox" id="rate3">3</div>
    <div class="ratingBox" id="rate4">4</div>
    <div class="ratingBox" id="rate5">5</div>
</div>

<div id="submitRating">Submit</div>